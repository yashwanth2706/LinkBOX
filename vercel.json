{
  "version": 2,
  "builds": [
    {
      "src": "urlmanager/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": ["staticfiles/**", "templates/**"]
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "urlmanager/wsgi.py" },
    { "src": "/(.*)", "dest": "urlmanager/wsgi.py" }
  ],
  "buildCommand": "bash build_files.sh"
}
